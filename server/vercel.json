{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "10mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "functions": {
        "server.js": {
            "maxDuration": 30,
            "memory": 1024
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}